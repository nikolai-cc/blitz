  "devDependencies": {
    "svelte": "next",
    "@sveltejs/kit": "latest",
    "@sveltejs/adapter-auto": "latest",
    "prettier": "latest",
    "prettier-plugin-svelte": "latest",
    "svelte-check": "latest",
		"typescript": "latest",
		"vite": "latest"
  },
  "type": "module"
}
